Job: "Ttres"
  Label: "t#bar{t} resonance"
  CmeLabel: "13 TeV"
  LumiLabel: "36 fb^{-1}"
  POI: "SigXsecOverSM"
  ReadFrom: HIST
  HistoPath: "."
