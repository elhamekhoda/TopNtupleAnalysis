Job: "JobTtres"
  Label: "t#bar{t} resonance"
  CmeLabel: "13 TeV"
  LumiLabel: "3.2 fb^{-1}"
  POI: "SigXsecOverSM"
  ReadFrom: HIST
  HistoPath: "."
  FitType: SPLUSB
  DebugLevel: 0
  SystControlPlots: TRUE
  HistoChecks: NOCRASH
  SystPruningNorm: 0.01
  MCstatThreshold: 0.01
  ImageFormat: eps

Fit: "FitTtres"
  FitType: SPLUSB


Region: "beM"
  Type: VALIDATION
  HistoName: "xlargeJetMboostede"
  VariableTitle: "Mass of the large-R jet [GeV]"
  Label: "Boosted e M"
  ShortLabel: "beM"
 
Region: "bmuM"
  Type: VALIDATION
  HistoName: "xlargeJetMboostedmu"
  VariableTitle: "Mass of the large-R jet [GeV]"
  Label: "Boosted #mu M"
  ShortLabel: "bmuM"

Region: "bePT"
  Type: VALIDATION
  HistoName: "xlargeJetPtboostede"
  VariableTitle: "pT of the large-R jet [GeV]"
  Label: "Boosted e pT"
  ShortLabel: "bePT"
  LogScale: TRUE
  BinWidth: 300
  
Region: "bmuPT"
  Type: VALIDATION
  HistoName: "xlargeJetPtboostedmu"
  VariableTitle: "pT of the large-R jet [GeV]"
  Label: "Boosted #mu pT"
  ShortLabel: "bmuPT"
  LogScale: TRUE
  BinWidth: 300


Region: "be"
